import java.util.Scanner ;
import java.util.StringTokenizer ;
import java.io.* ;

public class Student {
	
	public static char letterGrade(int score)
	{
		if(score >= 90)
			return 'A' ;
		else if(score >= 80 && score < 90)
			return 'B' ;
		else if(score >= 70 && score < 80)
			return 'C' ;
		else if(score >= 60 && score < 70)
			return 'D' ;
		else
			return 'F' ;
	}
	
	public static double calScore(int inputQuiz1 , int inputQuiz2 , int inputQuiz3 , int inputMidterm , int inputFinal)
	{
		double score = ((inputQuiz1 + inputQuiz2 + inputQuiz3) * 10.0 / 4) + (inputMidterm * 3.5 / 10) + (inputFinal * 0.4) ;
		return score ;
	}
	
	public static void main(String[] args) {
		Scanner input = null ;
		PrintWriter output = null ;
		
		try
		{
			input = new Scanner(new FileInputStream("scores.txt")) ;
			output = new PrintWriter(new FileOutputStream("gradeBook.txt")) ;
		}
		catch(FileNotFoundException e)
		{
			System.out.println("Fatal Error") ;
			System.exit(0) ;
		}
		
		input.useDelimiter(",") ;
		output.println("物件導向程式設計成績冊") ;
		String name = "" ;
		int quiz1 = 0 ;
		int quiz2 = 0 ;
		int quiz3 = 0 ;
		int midterm = 0 ;
		int finalExam = 0 ;
		int score = 0 ;
		
		while(input.hasNextLine())
		{
			name = input.next() ;
			quiz1 = input.nextInt() ;
			quiz2 = input.nextInt();
			quiz3 = input.nextInt();
			midterm = input.nextInt();
			finalExam = input.nextInt() ;
			input.nextLine() ;
			score = (int)calScore(quiz1 , quiz2 , quiz3 , midterm , finalExam) ;
			
			output.println(name + " " + "Quiz1 = " + quiz1 + " , Quiz2 = " + quiz2 + " , Quiz3 = " + quiz3 + " , Midterm = " + midterm + " , Final = " + finalExam + " , TotalScore = " + calScore(quiz1 , quiz2 , quiz3 , midterm , finalExam) + " , letterGrade = " + letterGrade(score)) ;
		}
		
		
		input.close() ;
		output.close() ;
	}

}
